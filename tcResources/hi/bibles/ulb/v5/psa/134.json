    "verseObjects": [
      {
        "type": "text",
        "text": "हे यहोवा के सब सेवकों, सुनो,\n"
      },
      {
        "tag": "q",
        "type": "quote",
        "text": "तुम जो रात-रात को यहोवा के भवन में खड़े रहते हो*,\n"
      },
      {
        "tag": "q",
        "type": "quote",
        "text": "यहोवा को धन्य कहो। (प्रका. 19:5)\n"
      },
      {
        "tag": "q",
        "type": "quote",
        "text": "\n"
      }
    ]
  },
  "2": {
    "verseObjects": [
      {
        "type": "text",
        "text": "अपने हाथ पवित्रस्‍थान में उठाकर,\n"
      },
      {
        "tag": "q",
        "type": "quote",
        "text": "यहोवा को धन्य कहो।\n"
      },
      {
        "tag": "q",
        "type": "quote",
        "text": "\n"
      },
      {
        "tag": "s5",
        "type": "section",
        "text": "\n"
      }
    ]
  },
  "3": {
    "verseObjects": [
      {
        "type": "text",
        "text": "यहोवा जो आकाश और पृथ्वी का कर्ता है,\n"
      },
      {
        "tag": "q",
        "type": "quote",
        "text": "वह सिय्योन से तुझे आशीष देवे।\n"
      },
      {
        "type": "text",
        "text": "\n"
      },
      {
        "tag": "s5",
        "type": "section",
        "text": "\n"
      }
    ]
  },
  "front": {
    "verseObjects": [
      {
        "tag": "s",
        "type": "section",
        "text": "स्तुति करने का आह्वान\n"
      },
      {
        "tag": "d",
        "content": "यात्रा का गीत\n"
      },
