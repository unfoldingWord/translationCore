  "1": {
    "verseObjects": [
      {
        "type": "text",
        "text": "Praise Yahweh!\n"
      },
      {
        "tag": "q1",
        "type": "quote",
        "text": "Praise God in his temple!\n"
      },
      {
        "tag": "q2",
        "type": "quote",
        "text": "Praise him who is in his fortress in heaven!\n"
      },
      {
        "tag": "q1",
        "nextChar": "\n",
        "type": "quote"
      }
    ]
  },
  "2": {
    "verseObjects": [
      {
        "type": "text",
        "text": "Praise him for the mighty deeds that he has performed;\n"
      },
      {
        "tag": "q2",
        "type": "quote",
        "text": "praise him because he is very great!\n"
      },
      {
        "type": "text",
        "text": "\n"
      },
      {
        "tag": "s5",
        "nextChar": "\n",
        "type": "section"
