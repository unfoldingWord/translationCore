  "55": {
    "verseObjects": [
      {
        "type": "text",
        "text": "The following descendants of King Solomon's servants returned to Jerusalem:\nSotai, Hassophereth, Peruda,\n"
      }
    ]
  },
  "56": {
    "verseObjects": [
      {
        "type": "text",
