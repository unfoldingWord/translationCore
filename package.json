{
  "name": "translationCore",
  "productName": "translationCore",
  "version": "0.8.1-rc.5",
  "manifestVersion": "1",
  "description": "A bridge between TS and TM",
  "main": "src/main.js",
  "scripts": {
    "start": "electron src/main.js",
    "update-version": "node ./scripts/versionScript.js",
    "lint": "eslint ./src",
    "test": "eslint ./src && jest",
    "release-osx": "npm i && gulp clean && npm i && npm run load-apps && gulp build --osx && gulp release --osx",
    "coverage": "jest --coverage",
    "load-apps": "git submodule update --init --recursive",
    "update-apps": "git submodule foreach --recursive git checkout master && git submodule foreach --recursive git pull",
    "install-electron-forge": "npm i -g electron-forge@3.2.0",
    "prettier-resource": "prettier --single-quote --print-width 80 --trailing-comma all --write 'tC_resources/**/*.json'"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageDirectory": "coverage",
    "testPathIgnorePatterns": [
      "tC_resources/*",
      "tC_apps/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klappy/translationCore.git"
  },
  "keywords": [
    "Translation",
    "Bible",
    "DSM",
    "Wycliffe",
    "unfoldingWord"
  ],
  "author": {
    "name": "unfoldingWord",
    "email": "info@unfoldingword.org"
  },
  "license": "GPL2",
  "bugs": {
    "url": "https://github.com/unfoldingWord-dev/translationCore/issues"
  },
  "homepage": "https://github.com/unfoldingWord-dev/translationCore#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "21.2.0",
    "electron": "1.7.9",
    "electron-packager": "9.1.0",
    "electron-prebuilt-compile": "1.6.11",
    "enzyme": "2.9.1",
    "eslint": "2.13.1",
    "eslint-plugin-jest": "21.3.2",
    "eslint-plugin-react": "7.4.0",
    "fetch-mock": "5.13.1",
    "glob": "7.1.2",
    "gulp": "3.9.1",
    "gulp-change": "1.0.0",
    "jest": "21.2.1",
    "jest-enzyme": "4.0.1",
    "ncp": "2.0.0",
    "prettier": "1.8.2",
    "react-addons-test-utils": "15.4.2",
    "react-test-renderer": "15.6.2",
    "redux-jest": "1.1.1",
    "redux-mock-store": "1.3.0",
    "redux-test-utils": "0.1.3",
    "spec-xunit-file": "0.0.1-3",
    "yargs": "10.0.3"
  },
