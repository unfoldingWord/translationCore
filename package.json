{
  "name": "translationCore",
  "productName": "translationCore",
  "version": "2.0.0",
  "minCompatibleVersion": "2.0.0",
  "manifestVersion": "7",
  "description": "A bridge between TS and TM",
  "main": "src/es6-init.js",
  "scripts": {
    "release-content-update": "node scripts/releaseContentUpdate",
    "find:extra-locale": "node scripts/locale/find_extras.js src/locale/English-en_US.json src/js",
    "find:missing-locale": "node scripts/locale/find_missing.js src/locale/English-en_US.json src/js",
    "start": "cross-env NODE_ENV=development electron src/es6-init.js -remote-debugging-port=9222",
    "start-with-logger": "REDUX_LOGGER=true npm run start",
    "update-version": "node ./scripts/versionScript.js",
    "locale-key-comparison": "node ./scripts/locale_key_comparison.js",
    "npm-clean-up": "rm -rf node_modules/ && rm -rf tC_apps/*/node_modules && rm package-lock.json && npm i",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "eslint ./src && jest",
    "ci-test": "eslint ./src && jest --coverage",
    "release-osx": "npm i && gulp clean && npm i && npm run load-apps && gulp build --osx && gulp release --osx",
    "coverage": "jest --coverage",
    "load-apps": "git submodule update --init --recursive",
    "update-apps": "git submodule foreach --recursive git checkout develop && git submodule foreach --recursive git pull",
    "install-electron-forge": "npm i -g electron-forge@3.2.0",
    "prettier-resource": "prettier --single-quote --print-width 80 --trailing-comma all --write 'tcResources/**/*.json'",
    "doc": "jsdoc -c jsdoc/jsdoc.json",
    "update-resources": "node scripts/resources/updateResources.js tcResources en hi el-x-koine hbo",
    "overwrite-resources": "rm -rf tcResources/en/bibles tcResources/en/translationHelps tcResources/el-x-koine tcResources/hi && npm run update-resources",
    "update-attributions": "node scripts/attributions/generateAttributionData.js src/js/components/home/license/attributionData.json"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!scripts/resources",
      "!src/main.js",
      "!src/locale"
    ],
    "coverageDirectory": "coverage",
    "setupFiles": [
      "./testSetUp/shim.js",
      "./testSetUp/enzyme.js"
    ],
    "snapshotSerializers": [
      "jest-serializer-path"
    ],
    "testPathIgnorePatterns": [
      "tcResources/*",
      "tC_apps/*"
    ],
    "rootDir": "./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klappy/translationCore.git"
  },
  "keywords": [
    "Translation",
    "Bible",
    "DSM",
    "Wycliffe",
    "unfoldingWord"
  ],
  "author": {
    "name": "unfoldingWord",
    "email": "info@unfoldingword.org"
